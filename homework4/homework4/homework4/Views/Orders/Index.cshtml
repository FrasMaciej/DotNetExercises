@model IEnumerable<Order>

@{
    ViewData["Title"] = "Lista zamówień";
}

<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoanDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReturnDate)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var loanCost in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => loanCost.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => loanCost.LoanDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => loanCost.ReturnDate)
            </td>
        </tr>

        }
    </tbody>

</table>
