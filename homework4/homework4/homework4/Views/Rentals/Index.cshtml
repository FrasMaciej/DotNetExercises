@model IEnumerable<Rental>

@{
    ViewData["Title"] = "Lista wypożyczalni";
}


<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Localization)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var rental in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => rental.Localization)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => rental.CreationDate)
                </td>
            </tr>

        }
    </tbody>

</table>
