@model IEnumerable<LoanCost>

@{
    ViewData["Title"] = "Cennik";
}

<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PricePerDay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CarQuality)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var loanCost in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => loanCost.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => loanCost.PricePerDay)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => loanCost.CarQuality)
                </td>
            </tr>

        }
    </tbody>

</table>
